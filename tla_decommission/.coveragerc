[run]
omit =
    # omit tests
    *tests*
    # omit interfaces
    *_interface.py
    # omit package init
    */__init__.py
    # omit test generator
    tests_generator.py
[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
